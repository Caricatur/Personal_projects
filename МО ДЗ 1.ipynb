        "def course_autumn(student):\n",
        "  if student['17_group'] != 0:\n",
        "    if student['17_group'] == 171 or student['17_group'] == 172 or student['17_group'] == 173:\n",
        "      return 2\n",
        "    else:\n",
        "      return 1\n",
        "  else:\n",
        "    if student['is_mi']:\n",
        "      return 2\n",
        "    else:\n",
        "      return 1"
      ]
